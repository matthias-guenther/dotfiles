#
# presentations
#
mux: tmux
name: presentations
dir: ~/bitbucket/presentations


[slides]
vim slides.md

[build]
rvm use ruby-2.3.0
rabbit -p --slides-per-page 1 -o slides.pdf slides.md

[pdf]
vim _config.yml

[pdf]
zathura slides.pdf &

[tmp]
tmux select-window -t slides
tmux kill-window -t tmp

